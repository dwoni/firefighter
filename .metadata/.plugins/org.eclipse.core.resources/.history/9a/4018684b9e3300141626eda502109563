package woni.FireFighter.Common;

import java.util.List;

import android.content.Context;

public interface MissionReceivedListener 
{
	void onClear(Context activity);
	void onItemLoaded(Context activity, Mission mission);
	void onItemsCompleted(Context activity);
    void onCompleted(Context activity, List<Mission> missions);
    void onFailed(Context activity, Exception exception);
}

